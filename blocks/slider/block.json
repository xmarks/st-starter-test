{
  "name": "st/slider",
  "title": "Slider Section",
  "description": "ST Starter Slider section.",
  "viewStyle": [ "file:./assets/css/view-style.min.css", "plugins.splidejs.core" ],
  "editorStyle": [ "file:./assets/css/editor-style.min.css", "plugins.splidejs.core" ],
  "viewScript": [ "file:./assets/js/viewScripts.js", "plugins.splidejs.splide" ],
  "editorScript": [ "file:./assets/js/editorScripts.js", "plugins.splidejs.splide" ],
  "category": "st-starter",
  "icon": "dashicons dashicons-slides",
  "keywords": [
    "slider, vertical slider, horizontal slider"
  ],
  "acf": {
    "mode": "preview",
    "renderTemplate": "block-render.php"
  },
  "example": {
    "attributes": {
      "preview": true,
      "data": {
        "preview_image_help": "block-image-preview.png"
      }
    }
  },
  "supports": {
    "align": [
      "full"
    ],
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ],
      "padding": [
        "top",
        "bottom"
      ],
      "units": [
        "px"
      ]
    },
    "anchor": true
  },
  "attributes": {
    "align": {
      "type": "string",
      "default": "full"
    }
  }
}
